    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.3.1",
    "express": "^4.16.3",
    "express-session": "^1.16.2",
    "if-env": "^1.0.4",
    "mongoose": "^5.6.8",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react-bootstrap": "^1.0.0-beta.10",
    "react-spring": "^8.0.27",
    "reactstrap": "^5.0.0-alpha.4"
  },
